<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/saveImg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="Cost/kW:"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/kwHighCost"
                android:layout_width="40dp"
                android:layout_height="45dp"
                android:layout_marginStart="2dp"
                android:backgroundTint="@color/TextColor1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="24dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="Time High Cost:"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="from"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/fromHighView"
                android:layout_width="40dp"
                android:layout_height="45dp"
                android:layout_marginStart="2dp"
                android:backgroundTint="@color/TextColor1"
                android:ems="10"
                android:inputType="number"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="24dp" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="to"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/toHighView"
                android:layout_width="40dp"
                android:layout_height="45dp"
                android:layout_marginStart="2dp"
                android:backgroundTint="@color/TextColor1"
                android:ems="10"
                android:inputType="number"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="24dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="Cost/kW:"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/kwLowCost"
                android:layout_width="40dp"
                android:layout_height="45dp"
                android:layout_marginStart="2dp"
                android:backgroundTint="@color/TextColor1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="24dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="Time Low Cost: "
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="from"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/fromLowView"
                android:layout_width="40dp"
                android:layout_height="45dp"
                android:layout_marginStart="2dp"
                android:backgroundTint="@color/TextColor1"
                android:ems="10"
                android:inputType="number"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="24dp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="to"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="46dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/toLowView"
                android:layout_width="40dp"
                android:layout_height="45dp"
                android:layout_marginStart="2dp"
                android:backgroundTint="@color/TextColor1"
                android:ems="10"
                android:inputType="number"
                android:textColor="@color/TextColor1"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="24dp" />

        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/saveImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/filename_save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

</android.support.constraint.ConstraintLayout>